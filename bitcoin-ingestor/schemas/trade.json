{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Binance Trade Event",
  "description": "Schema for Binance WebSocket trade stream events",
  "type": "object",
  "required": [
    "e",
    "E",
    "s",
    "t",
    "p",
    "q",
    "T",
    "m"
  ],
  "properties": {
    "e": {
      "type": "string",
      "description": "Event type",
      "const": "trade"
    },
    "E": {
      "type": "integer",
      "description": "Event time (milliseconds)"
    },
    "s": {
      "type": "string",
      "description": "Symbol (e.g., BTCUSDT)"
    },
    "t": {
      "type": "integer",
      "description": "Trade ID"
    },
    "p": {
      "type": "string",
      "description": "Price"
    },
    "q": {
      "type": "string",
      "description": "Quantity"
    },
    "b": {
      "type": "integer",
      "description": "Buyer order ID"
    },
    "a": {
      "type": "integer",
      "description": "Seller order ID"
    },
    "T": {
      "type": "integer",
      "description": "Trade time (milliseconds)"
    },
    "m": {
      "type": "boolean",
      "description": "Is the buyer the market maker?"
    },
    "M": {
      "type": "boolean",
      "description": "Ignore (deprecated)"
    }
  },
  "examples": [
    {
      "e": "trade",
      "E": 1672531200000,
      "s": "BTCUSDT",
      "t": 12345,
      "p": "16500.00",
      "q": "0.01000000",
      "b": 88,
      "a": 50,
      "T": 1672531200000,
      "m": true,
      "M": true
    }
  ]
}
